{
  "name": "ionic-gib",
  "version": "0.0.1",
  "author": "William Raiford",
  "repository": {
    "url": "https://github.com/wraiford/ibgib",
    "type": "git",
    "directory": "ionic-gib"
  },
  "funding": {
    "type": "individual",
    "url": "https://paypal.me/ibGib"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl",
    "build": "ng build --output-path www",
    "build:aot": "ng build --aot --output-path www",
    "watch": "ng build --aot --output-path www --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ff:createprofile": "/Applications/Firefox.app/Contents/MacOS/firefox -CreateProfile \"webext-test\"",
    "build:ff": "npm run build:aot",
    "build:chrome": "npm run build:aot",
    "ff:chrome": "web-ext run -s './www' -t firefox-desktop -t chromium --keep-profile-changes --browser-console",
    "ff": "web-ext run -s './www' -t firefox-desktop --firefox-profile=webext-test --keep-profile-changes --browser-console",
    "ff:noprofile": "web-ext run -s './www' -t firefox-desktop --browser-console",
    "chrome": "web-ext run -s './www' -t chromium --keep-profile-changes --browser-console",
    "start:android": "ionic capacitor run android",
    "build:android": "ionic build && ionic capacitor sync android",
    "watch:android": "ionic capacitor run android --live-reload",
    "sync:android": "ionic capacitor sync android",
    "sync": "ionic capacitor sync"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^13.1.2",
    "@angular/common": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@aws-sdk/client-dynamodb": "^3.43.0",
    "@capacitor/android": "^2.2.1",
    "@capacitor/core": "2.2.1",
    "@ionic-native/core": "^5.0.7",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^6.0.1",
    "@ionic/angular-server": "^6.0.1",
    "@ionic/pwa-elements": "^1.5.2",
    "encrypt-gib": "^0.1.2",
    "lex-gib": "^0.10.1",
    "rxjs": "~6.5.1",
    "ts-gib": "^0.3.9",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "^13.1.2",
    "@angular/compiler": "~13.1.1",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/language-service": "~13.1.1",
    "@capacitor/cli": "2.2.1",
    "@ionic/angular-toolkit": "^3.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.11.11",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "4.4"
  },
  "description": "An Ionic project"
}

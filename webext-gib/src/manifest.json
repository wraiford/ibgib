{
    "name": "webext-gib",
    "version": "0.0.1",
    "description": "web extensions angular app.",
    "browser_action": {
        "default_popup": "index.html?target=sidebar",
        "default_title": "ibgib",
        "default_icon": {
            "32": "/favicon.ico",
            "64": "/favicon.ico"
        }
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
    "permissions": [
        "storage", 
        "tabs"
    ],
    "manifest_version": 2
}